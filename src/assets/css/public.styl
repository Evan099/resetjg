  //table右对齐

.tableToRight tr td:nth-of-type(1){
  text-align right
}
.tableToRight tr td:nth-of-type(3){
  text-align right
}





//去掉table el-input 的border
.tableBorderGone>tr>td input{
  border none
}
.tableBorderGone>tr>td textarea{
  border none
}
//去掉颜色后加个颜色区分，和  tableBorderGone搭配使用
.tableColor>tr>td input,textarea:nth-of-type(even){
  color steelblue
}
.tableColor>tr>td textarea{
  color steelblue
}

//table里面点击图片查看大图
.productImg {
  width 100%;
  height:60px;
  cursor:pointer;
}

