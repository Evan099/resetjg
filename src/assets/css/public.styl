//创建项目
.createAlert {
  width: 25%;
  margin-bottom: 2px;
  display: inline-block;
  margin-left: 20px;
  height 100%;
  line-height 100%;
  margin-right 0;
  float: right
}
.keyTieleMsg{
  background-color: #DAE9F7;
  color: #135294;
  padding: 5px;
  font-size 14px;
  text-align center
}
.creatProject {
  font-size 14px
  border: solid 1px #D3E3F3;
  min-width: 1100px;
  margin 10px
  margin-top 0
}

.creatProject-title {
  height: 40px;
  line-height: 40px;
  border-bottom: solid 1px #D3E3F3;
}

.creatProject-title > span {
  display: inline-block;
  padding: 0 8px;
  margin-left: 20px;
  text-align: center;
  height: 100%;
  color: #39618F;
  border-bottom: solid 2px #39618F;
  box-sizing: border-box;
}

.creatProject-main {
  width: 100%;
  height: auto;
  margin-top: 15px;
}

.creatProject-chose {
  width: 100%;
  height: 110px;
  background: #D3E3F3;
  font-size 14px
}

.projectTable {
  width: 100%;
}

.projectTable tr td {
  text-align: right;
  padding-right: 15px;
  line-height: 50px;
}

.projectTable2 {
  width: 100%;
  font-size 14px
}

.projectTable2 tr td {
  line-height: 50px;
}

.projectTable2 tr td:nth-of-type(1) {
  width: 130px;
  text-align: right;
  padding-right: 20px;
}

.projectTable2 tr td:nth-of-type(3) {
  width: 130px;
  text-align: right;
  padding-right: 20px;
}

.creatProject-handChose {
  width: 100%;
  height: 30px;
  margin-top: 15px;
}

.creatProject-write {
  width: 100%;
  height: auto;
  padding-bottom: 30px;
}

